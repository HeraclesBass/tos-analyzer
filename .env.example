# Gemini API
GEMINI_API_KEY="your-gemini-api-key-here"

# Database (PostgreSQL)
DATABASE_URL="postgresql://user:password@localhost:5432/dbname?schema=tos_analyzer"

# Redis
REDIS_URL="redis://:password@localhost:6379"

# Security
SESSION_SALT="generate-a-random-string-here"

# Application
PORT=8101
NODE_ENV="production"
NEXT_PUBLIC_APP_URL="https://fine-print.org"

# Rate Limiting
RATE_LIMIT_PER_MINUTE=10

# Budget Protection (Gemini tokens per day, default 5M ~ $10-15/day max)
DAILY_TOKEN_BUDGET=5000000

# Analytics
ANALYTICS_ENABLED=true

# PDF Processing
MAX_FILE_SIZE_MB=10
